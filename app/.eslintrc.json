{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", {
            "arrays": "never"
        }],
        "no-plusplus": ["error", {
            "allowForLoopAfterthoughts": true
        }],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
